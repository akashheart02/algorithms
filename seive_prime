/************prime number generation Seive Algorithm********/

#include <iostream>

using namespace std;
#define MAX 10000000
int A[MAX];
int prime[MAX];
int t;
void prime_seive()
{
   A[0]=1;
   A[1]=1;  
   for(int i=2;i<=MAX;i+=2)
      A[i]=1;
      
   for(int i=3;i*i<=MAX;i+=2)
     {
        if(!A[i])
          {
            for(int j=i*i;j<=MAX;j+=i+i)
              A[j]=1;
          }
     }
     
   for(int i=2;i<=MAX;i++)
     if(!A[i])
       prime[t++]=i;
}

int main()
{
   prime_seive();
   for(int i=0;i<t;i++)
      printf("%d\n",prime[i]);
      
system("pause");
return 0;
}       
    
                             
